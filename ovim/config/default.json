        },
        {
            "repo": "roxma/vim-hug-neovim-rpc",
            "if": "!has('nvim')"
        },

        {
            "repo": "vim-airline/vim-airline",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/airline.vim"
        },
        {
            "repo": "vim-airline/vim-airline-themes"
        },
        {
            "repo": "Yggdroot/indentLine",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/indentLine.vim"
        },
        {
            "repo": "scrooloose/nerdtree",
            "on": "NERDTreeToggle",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/nerdtree.vim"
        },
        {
            "repo": "scrooloose/nerdcommenter"
        },
        {
            "repo": "majutsushi/tagbar",
            "on": "TagbarToggle",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/tarbar.vim"
        },
        {
            "repo": "jiangmiao/auto-pairs",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/auto-pairs.vim"
        },
        {
            "repo": "SirVer/ultisnips",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/ultisnips.vim"
        },
        {
            "repo": "honza/vim-snippets"
        },
        {
            "repo": "chiel92/vim-autoformat",
            "on": "Autoformat",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/autoformat.vim"
        },
        {
            "repo": "skywind3000/asyncrun.vim",
            "hook_source":"source $OVIM_ROOT_PATH/plugins/asyncrun.vim"
        },
        {
