{
    "hook_source": "",
    "plugins": [
        {
            "repo": "arakashic/nvim-colors-solarized"
        },
        {
            "repo": "morhetz/gruvbox"
        },
        {
            "repo": "miconda/lucariox.vim"
        },
        {
